{
	"name": "wedgehr/laravel-common-password-validator",
	"description": "Protect your users from using the most common passwords",
	"keywords": [
		"wedge",
		"wedgehr",
		"password",
		"validator"
	],
	"homepage": "https://github.com/wedgehr/laravel-common-password-validator",
	"license": "MIT",
	"require": {
		"php": "^8.0",
		"illuminate/support": "^8.0",
		"illuminate/database": "^8.0",
		"illuminate/container": "^8.0"
	},
	"autoload": {
		"psr-4": {
			"Wedge\\Validators\\CommonPassword\\": "src"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Wedge\\Validators\\CommonPassword\\ServiceProvider"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
